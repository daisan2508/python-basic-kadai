      "source": [
        "# 与えられた引数$priceに消費税を加算し、その値を出力する関数を定義する\n",
        "def calculate_total(price):\n",
        "\n",
        "    # 与えられた引数priceに消費税を加算し、変数totalに代入する\n",
        "    total = price + (price * 10 / 100)\n",
        "\n",
        "    # 変数totalの値を出力する\n",
        "    print(f\"{total}円\")\n",
        "\n",
        "# 関数を呼び出し、引数として購入金額を渡す\n",
        "calculate_total(100)"
      ]
